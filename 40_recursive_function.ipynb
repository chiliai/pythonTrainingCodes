   "source": [
    "# factorial with while\n",
    "n = 8\n",
    "res = 1\n",
    "i = n\n",
    "while(i >= 1):\n",
    "    res *= i \n",
    "    i -= 1\n",
    "\n",
    "print(res)\n",
    "    "
